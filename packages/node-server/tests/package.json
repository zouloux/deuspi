{
  "name": "node-server-test",
  "version": "1.0.0",
  "description": "",
  "main": "server.config.js",
  "type": "module",
  "scripts": {
    "-- VITE --": "",
    "vite-dev": "vite dev --host",
    "vite-build": "vite build --emptyOutDir",
    "-- SERVER --": "",
    "server-dev": "node server.config.js --dev",
    "server-build": "node server.config.js",
    "server-start": "cd dist/server/ && node server.js",
    "-- BOTH --": "",
    "dev": "clear && (npm run vite-dev & (sleep .6 && npm run server-dev) & wait)",
    "build": "npm run vite-build && npm run server-build"
  },
  "author": "Alexis Bouhet ( ZoulouX )",
  "license": "MIT",
  "dependencies": {
    "esbuild": "^0.17.5",
    "vite": "^4.1.0"
  }
}
